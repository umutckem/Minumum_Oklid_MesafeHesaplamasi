import math

# Noktaları temsil eden liste
points = [ (1, 2), (4, 6), (7, 8), (10, 12) ]

# Öklid mesafesi hesaplama fonksiyonu
def euclideanDistance(point1, point2):
    return math.sqrt((point2[0] - point1[0])**2 + (point2[1] - point1[1])**2)

# Mesafeleri saklayacak liste
distances = []

# Noktalar arasındaki mesafeleri hesaplayın
for i in range(len(points)):
    for j in range(i + 1, len(points)):
        distance = euclideanDistance(points[i], points[j])
        distances.append(distance)

# Minimum mesafeyi bulun
min_distance = min(distances)

# Sonuçları yazdırın
print("Mesafeler:", distances)
print("Minimum Mesafe:", min_distance)
